
entity Question {
	/**
	* The firstname attribute.
	*/
	text String,
	category String,
	isForQuizz Boolean,
    answers String
}

entity AnswerChoice {
	label String
}

entity Answer {
  userId String,
  answerChoiseId String
}

paginate Question with pagination
paginate Answer with pagination
paginate AnswerChoice with pagination

service all with serviceImpl
